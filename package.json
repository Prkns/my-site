{
  "name": "gatsby-starter-mdx-plus",
  "description": "Gatsby MDX starter with markdown images, tailwind css and semi sticky auto generated navigation",
  "version": "1.0.1",
  "author": "Tim Gilpin",
  "dependencies": {
    "@mdx-js/mdx": "^0.16.6",
    "@mdx-js/tag": "^0.16.6",
    "gatsby": "^2.0.83",
    "gatsby-image": "^2.0.20",
    "gatsby-mdx": "^0.3.2",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-offline": "^2.0.20",
    "gatsby-plugin-react-helmet": "^3.0.5",
    "gatsby-plugin-sass": "^2.2.1",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-sharp": "^2.4.2",
    "node-sass": "^4.13.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-headroom": "^2.2.4",
    "react-helmet": "^5.2.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build:css": "tailwind build ./src/components/index.tailwind.css -c ./tailwind.js -o ./src/components/layout.css",
    "build": "npm run build:css && gatsby build",
    "develop": "npm run build:css && gatsby develop",
    "start": "npm run develop",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "prettier": "^1.15.2",
    "tailwindcss": "^0.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tjgilpin/gatsby-starter-mdx-plus"
  }
}
